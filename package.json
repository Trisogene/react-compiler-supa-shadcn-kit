{
	"name": "rc-supa-shad-kit",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"init": "pnpm install && npx supabase login && npx supabase link",
		"fe:start": "vite",
		"fe:build": "tsc -b && vite build",
		"check": "biome check --write --unsafe",
		"be:start": "npx supabase start",
		"be:stop": "npx supabase stop",
		"be:reset": "npx supabase db reset",
		"be:types": "npx supabase gen types typescript --local > src/types/supabase.d.ts",
		"be:diff": "npx supabase db diff -f",
		"be:push": "npx supabase db push",
		"be:pull": "npx supabase db pull"
	},
	"dependencies": {
