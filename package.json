		"clsx": "^2.1.1",
		"cmdk": "^1.1.1",
		"date-fns": "^4.1.0",
		"embla-carousel-react": "^8.6.0",
		"input-otp": "^1.4.2",
		"lucide-react": "^0.548.0",
		"next-themes": "^0.4.6",
		"radix-ui": "latest",
		"react": "^19.2.0",
		"react-day-picker": "^9.11.1",
